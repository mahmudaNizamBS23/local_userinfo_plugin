<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/userinfo/db" VERSION="20200213" COMMENT="XMLDB file for local userinfo"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_userinfo" COMMENT="each record is one folder resource">
      <FIELDS>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="varchar" LENGTH="100" NOTNULL="true"  SEQUENCE="false"/>
        <FIELD NAME="email" TYPE="varchar" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="phoneno" TYPE="int" NOTNULL="true" SEQUENCE="false"/>
        </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="userid"/>
      </KEYS>
      </TABLE>
  </TABLES>
</XMLDB>
